{
  "name": "@eduardofurihata/eduzz-mcp",
  "version": "1.0.3",
  "description": "Eduzz MCP Suite - Knowledge base, API client, and configuration management",
  "private": false,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eduardofurihata/eduzz-mcp-private.git",
    "directory": "eduzz-mcp"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "test": "npm run test --workspaces",
    "lint": "npm run lint --workspaces",
    "clean": "npm run clean --workspaces",
    "sync": "npm run sync -w @eduzz/mcp-knowledge",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "packages/*/dist/**",
    "packages/*/package.json",
    "packages/*/README.md",
    "packages/mcp-knowledge/data/**"
  ],
  "keywords": [
    "eduzz",
    "mcp",
    "model-context-protocol",
    "ai",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
